let money = 96;
print("Now you have 96 RMB");
let price = 59;

let check = func(money,price) {
    if (money > price) {
        print("OK, you can buy this");
        return true;
    }
    else {
        print("Sorry, you have no enough money");
        return false;
    }
}

if (check(money,price)) {
    let money = money - price;
}
print("Now you have",money,"RMB");
